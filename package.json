{
  "name": "@gaincompliance/scaffolder-sub-command",
  "description": "scaffolder sub-command for commander",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "GainCompliance/scaffolder-sub-command",
  "bugs": "https://github.com/GainCompliance/scaffolder-sub-command/issues",
  "homepage": "https://npm.im/@gaincompliance/scaffolder-sub-command",
  "author": "Gain Compliance (https://github.com/GainCompliance)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis lint --no-interactive",
    "test": "run-s lint:* test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@gaincompliance/eslint-config-gain": "^1.0.0",
    "@travi/any": "1.8.2",
    "babel-preset-travi": "1.4.0",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "3.1.0",
    "commitlint-config-gain": "1.0.6",
    "cz-conventional-changelog": "2.1.0",
    "greenkeeper-lockfile": "^2.5.0",
    "husky": "^1.0.0-rc.8",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "nyc": "^13.0.1",
    "rimraf": "2.6.2",
    "rollup": "0.66.6",
    "sinon": "^6.1.5"
  },
  "dependencies": {
    "@travi/github-scaffolder": "1.0.0",
    "@travi/javascript-scaffolder": "3.12.4",
    "@travi/project-scaffolder": "5.0.1",
    "@travi/travis-scaffolder-javascript": "1.4.3"
  }
}
