{
  "name": "@gaincompliance/scaffolder-sub-command",
  "description": "scaffolder sub-command for commander",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "GainCompliance/scaffolder-sub-command",
  "bugs": "https://github.com/GainCompliance/scaffolder-sub-command/issues",
  "homepage": "https://npm.im/@gaincompliance/scaffolder-sub-command",
  "author": "Gain Compliance (https://github.com/GainCompliance)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "remark --frail .",
    "lint:travis": "travis lint --no-interactive",
    "test": "run-s lint:* test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@gaincompliance/eslint-config-gain": "^1.0.0",
    "@gaincompliance/remark-preset-lint": "^1.0.0",
    "@travi/any": "1.8.3",
    "babel-preset-travi": "1.4.2",
    "babel-register": "6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "7.1.1",
    "codecov": "3.2.0",
    "commitlint-config-gain": "1.0.11",
    "cz-conventional-changelog": "2.1.0",
    "husky": "^1.1.2",
    "mocha": "6.0.2",
    "npm-run-all": "4.1.5",
    "nyc": "^13.1.0",
    "remark": "^10.0.1",
    "remark-cli": "^6.0.1",
    "rimraf": "2.6.3",
    "rollup": "1.2.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "sinon": "^7.0.0"
  },
  "dependencies": {
    "@gaincompliance/python-scaffolder": "1.0.0-alpha.1",
    "@travi/github-scaffolder": "3.0.3",
    "@travi/javascript-scaffolder": "5.0.0",
    "@travi/node-app-engine-standard-scaffolder": "1.0.0-alpha.1",
    "@travi/project-scaffolder": "6.0.2",
    "@travi/travis-scaffolder-javascript": "1.10.0"
  }
}
